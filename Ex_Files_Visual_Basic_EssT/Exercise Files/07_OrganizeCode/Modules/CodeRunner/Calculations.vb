Module Calculations
    Public Function AddValue(dbl1 As Double, dbl2 As Double) As Double
        Return dbl1 + dbl2
    End Function

    Public Function SubValue(dbl1 As Double, dbl2 As Double) As Double
        Return dbl1 - dbl2
    End Function

    Public Function MulValue(dbl1 As Double, dbl2 As Double) As Double
        Return dbl1 * dbl2
    End Function

    Public Function DivValue(dbl1 As Double, dbl2 As Double) As Double
        Return dbl1 / dbl2
    End Function
End Module
